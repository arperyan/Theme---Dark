{
  "_inherit": true,
  "_variables": {
    "@TitleSize": "14px",
    "@TitleColor": "#ffffff",
    "@SubtitleSize": "11px",
    "@SubtitleColor": "#ffffff",
    "@TextSize": "12px",
    "@TextColor": "#ffffff",
    "@ChartTitleSize": "11px",
    "@ChartTitleColor": "#ffffff",
    "@LabelSize": "13px",
    "@LabelColor": "#ffffff",
    "@FooterSize": "10px",
    "@FooterColor": "#ffffff",
    "@BackgroundColor": "#293741",
    "@AxisMajorColor": "#008f11",
    "@AxisMinorColor": "#008f11",
    "@Aluminium": "#a7a8aa",
    "@Concrete" : "#d0d0ce",
    "@Steel": "#63666a"
  },
  "color": "@TextColor",
  "fontSize": "@TextSize",
  "backgroundColor": "@BackgroundColor",
  "fontFamily": "Arial",
  "object": {
    "title": {
      "main": {
        "color": "@TitleColor",
        "fontSize": "@TitleSize"
      },
      "subTitle": {
        "color": "@SubtitleColor",
        "fontSize": "@SubtitleSize"
      },
      "footer": {
        "color": "@FooterColor",
        "fontSize": "@FooterSize",
        "backgroundColor": "@BackgroundColor"
      }
    },
    "label": {
      "name": {
        "color": "@LabelColor",
        "fontSize": "@LabelSize"
      },
      "value": {
        "color": "@TextColor",
        "fontSize": "@TextSize"
      }
    },
    "axis": {
      "title": {
        "color": "@ChartTitleColor",
        "fontSize": "@ChartTitleSize"
      },
      "label": {
        "name" : {
          "color": "@ChartTitleColor",
          "fontSize": "@ChartTitleSize"
        }
      },
      "line": {
        "major": {
          "color": "@Aluminium"
        },
        "minor": {
          "color": "@Aluminium"
        }
      }
    },
    "grid": {
      "line": {
        "highContrast": {
          "color": "#eeeeee"
        },
        "major": {
          "color": "@Aluminium"
        },
        "minor": {
          "color": "@Concrete"
        }
      }
    },
    "referenceLine": {
      "label": {
        "name": {
          "color": "@TitleColor",
          "fontSize": "@TitleSize"
        }
      },
      "outOfBounds": {
        "color": "@TextColor",
        "backgroundColor": "#ffffff",
        "fontSize": "@TextSize"
      }
    },
    "legend": {
      "title": {
        "color": "@TextColor",
        "fontSize": "@TextSize"
      },
      "label": {
        "color": "@ChartTitleColor",
        "fontSize": "@ChartTitleSize"
      }
    },
    "barChart": {
      "label": {
        "value": {
          "color": "@TextColor",
          "fontSize": "@TextSize"
        }
      },
      "outOfRange": {
        "color": "@TextColor"
      }
    },
    "boxPlot": {
     "box": {
       "whisker": {
         "stroke": "@Concrete"
       },
       "line": {
         "stroke": "@Concrete"
       },
       "box": {
         "fill": "@Concrete",
         "stroke": "@Concrete"
       }
     }
    },
    "distributionPlot": {
     "box": {
       "fill": "@Concrete"
     }
    },
    "filterpane": {
     "title": {
       "main": {
         "color": "@TitleColor",
         "fontSize": "@TitleSize"
       }
     }
    },
    "kpi": {
      "backgroundColor": "@BackgroundColor",
      "title": {
        "main": {
          "color": "@TitleColor",
          "fontSize": "@TitleSize"
        },
        "subTitle": {
          "color": "@SubtitleColor",
          "fontSize": "@SubtitleSize"
        },
        "footer": {
          "color": "@FooterColor",
          "fontSize": "@FooterSize",
          "backgroundColor": "@BackgroundColor"
        }
      }
    },
    "gauge": {
     "label": {
       "value": {
         "color": "@LabelColor",
         "fontSize": "@LabelSize"
       }
     }
    },
    "histogram": {
     "label": {
       "value": {
         "color": "@TextColor",
         "fontSize": "@TextSize"
       }
     }
    },
    "lineChart": {
      "outOfRange": {
        "color": "@Concrete"
      },
      "label": {
        "value": {
          "color": "@TextColor",
          "fontSize": "@TextSize"
        }
      }
    },
    "listBox": {
     "title": {
       "main": {
         "color": "@TitleColor",
         "fontSize": "@TitleSize"
       }
     },
     "content": {
       "color": "@TextColor",
       "fontSize": "@TextSize"
     }
    },
    "mapChart": {
     "label": {
       "value": {
         "color": "@TextColor",
         "fontSize": "@TextSize"
       }
     },
     "legend": {
       "title": {
         "color": "@TextColor"
       },
       "label": {
         "color": "@TextColor"
       }
     }
    },
    "pieChart": {
      "axis": {
        "title": {
          "color": "@TitleColor",
          "fontSize": "@TitleSize"
        }
      },
      "label": {
        "name": {
          "color": "@TitleColor",
          "fontSize": "@TitleSize"
        },
        "value": {
          "color": "@TextColor",
          "fontSize": "@TextSize"
        }
      }
    },
    "pivotTable": {
     "header" : {
       "color": "@LabelColor",
       "fontSize": "@LabelSize"
     },
     "content": {
       "fontSize": "@TextSize",
       "color": "@TextColor"
     }
    },
    "scatterPlot": {
     "label": {
       "value": {
         "color": "@TextColor",
         "fontSize": "@TextSize"
       }
     }
    },
    "straightTable" : {
     "header" : {
       "color": "@LabelColor",
       "fontSize": "@LabelSize"
     },
     "content": {
       "fontSize": "@TextSize",
       "color": "@TextColor"
     }
    },
    "treemap": {
     "branch": {
       "backgroundColor": "#ffffff",
       "label": {
         "color": "@TextColor",
         "fontSize": "@TextSize"
       }
     },
     "leaf": {
       "label": {
         "fontSize": "@TextSize",
         "color": "@TextColor"
       }
     }
    },
    "waterfallChart": {
     "label": {
       "value": {
         "color": "@TextColor",
         "fontSize": "@TextSize"
       }
     },
     "value": {
       "color": {
         "default": "@Concrete",
         "dark": "@Steel",
         "light": "@Concrete"
       }
     },
     "shape": {
       "positiveValue": {
         "fill": "@TitleColor"
       },
       "negativeValue": {
         "fill": "@Steel"
       },
       "subtotal": {
         "fill": "@Aluminium"
       },
       "bridge": {
         "stroke": "@Concrete"
       }
     }
   },
   "textImage": {
        "backgroundColor": "@BackgroundColor",
        "title": {
          "main": {
            "color": "@TitleColor",
            "fontSize": "@TitleSize"
          },
          "subTitle": {
            "color": "@SubtitleColor",
            "fontSize": "@SubtitleSize"
          },
          "footer": {
            "color": "@FooterColor",
            "fontSize": "@FooterSize",
            "backgroundColor": "@BackgroundColor"
          }
        }
      }
  },
